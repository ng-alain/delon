{
  "name": "delon",
  "version": "1.3.3",
  "typings": "index.d.ts",
  "description": "Delon is a set of essential modules for ng-alain.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cipchk/delon.git"
  },
  "keywords": [
    "delon",
    "ng-alain",
    "alain",
    "antd",
    "ng-zorro-antd"
  ],
  "author": "cipchk <cipchk@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cipchk/delon/issues"
  },
  "homepage": "http://ng-alain.com",
  "scripts": {
    "precommit": "npm run lint-staged",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "prepush": "npm run lint",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tslint -p tsconfig.json -c tslint.json 'packages/**/*.ts'",
    "lint:style": "stylelint \"{packages}/**/*.less\" --syntax less",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint-staged": "lint-staged",
    "test": "ng test --code-coverage --watch=false",
    "test:schematics": "bash build-schematics.sh -b -t",
    "site:gen": "node scripts/site/main",
    "site:release": "bash ./site.sh -b",
    "site:gh": "bash ./site.sh -b -d",
    "release": "bash ./build.sh && bash build-schematics.sh -b",
    "publish:next": "npm run release && bash ./scripts/release/publish-next",
    "publish": "npm run release && bash ./scripts/release/publish && bash ./scripts/release/sync-taobao",
    "scaffold:release": "bash ./scaffold.sh -b",
    "scaffold:gh": "bash ./scaffold.sh -b -d"
  },
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@antv/data-set": "^0.8.8",
    "@antv/g2": "^3.0.11",
    "@antv/g2-plugin-slider": "^2.0.2",
    "@ngx-translate/core": "^10.0.1",
    "@stackblitz/sdk": "^1.2.0",
    "ajv": "^6.4.0",
    "core-js": "^2.5.4",
    "enquire.js": "^2.1.6",
    "extend": "^3.0.1",
    "file-saver": "^1.3.8",
    "marked": "^0.3.19",
    "ng-github-button": "^1.0.0",
    "ng-zorro-antd": "^1.4.0",
    "ng2-ace-editor": "^0.3.8",
    "ngx-color": "^1.3.1",
    "ngx-countdown": "^3.0.0",
    "ngx-highlight-js": "^2.0.0",
    "ngx-simplemde": "^1.1.1",
    "ngx-tinymce": "^2.0.0",
    "ngx-ueditor": "^2.1.0",
    "qrious": "^4.0.2",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular-devkit/core": "~0.7.0",
    "@angular-devkit/schematics": "~0.7.0",
    "@angular/cli": "~6.1.1",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@schematics/schematics": "~0.7.0",
    "@types/deep-extend": "^0.4.31",
    "@types/extend": "^3.0.0",
    "@types/fs-extra": "^5.0.1",
    "@types/htmlparser2": "^3.7.31",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jszip": "^3.1.3",
    "@types/mockjs": "^1.0.0",
    "@types/node": "~8.9.4",
    "antd-theme-generator": "^1.0.7",
    "codecov": "^3.0.2",
    "codelyzer": "~4.2.1",
    "domhandler": "^2.4.2",
    "editorconfig-tools": "^0.1.1",
    "fs-extra": "^6.0.0",
    "gh-pages": "^1.1.0",
    "htmlparser2": "^3.9.2",
    "husky": "^0.14.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jsonml.js": "^0.1.0",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "klaw-sync": "^4.0.0",
    "lint-staged": "^7.1.0",
    "mark-twain": "^2.0.3",
    "mockjs": "^1.0.1-beta3",
    "mustache": "^2.3.0",
    "prettier": "^1.12.1",
    "prettier-stylelint": "^0.4.2",
    "protractor": "~5.3.0",
    "rollup": "^0.58.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^3.0.0",
    "stream": "0.0.2",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "2.9.x",
    "webpack-bundle-analyzer": "^2.11.2",
    "xlsx": "^0.12.12",
    "less": "~2.7.0",
    "less-plugin-clean-css": "^1.5.1"
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "packages/**/*.ts": [
      "npm run lint:ts",
      "git add"
    ]
  }
}
