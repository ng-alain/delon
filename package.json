{
  "name": "delon",
  "version": "2.0.0-rc.1",
  "typings": "index.d.ts",
  "description": "Delon is a set of essential modules for ng-alain.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng-alain/delon.git"
  },
  "keywords": [
    "delon",
    "ng-alain",
    "alain",
    "antd",
    "ng-zorro-antd",
    "angular"
  ],
  "author": "cipchk <cipchk@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ng-alain/ng-alain/issues"
  },
  "homepage": "https://ng-alain.com",
  "scripts": {
    "lint-staged": "lint-staged",
    "lint": "bash scripts/ci/lint.sh",
    "test": "ng test --code-coverage --watch=false && bash scripts/ci/build-schematics.sh -b -t",
    "release": "bash scripts/ci/build-lib.sh && bash scripts/ci/build-schematics.sh -b",
    "site:gen": "node scripts/site/main init",
    "site": "npm run site:gen && ng serve",
    "site:build": "npm run site:gen && ng build --prod --build-optimizer --base-href /",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --pkg package.json",
    "pre-publish": "node scripts/publish/publish.js && node scripts/publish/publish-scaffold.js",
    "publish:next": "bash scripts/publish/publish.sh -next"
  },
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/elements": "^6.0.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@antv/data-set": "^0.9.5",
    "@antv/g2": "^3.2.7",
    "@antv/g2-plugin-slider": "^2.0.2",
    "@ngx-translate/core": "^10.0.1",
    "@stackblitz/sdk": "^1.2.0",
    "ajv": "^6.4.0",
    "core-js": "^2.5.4",
    "enquire.js": "^2.1.6",
    "extend": "^3.0.1",
    "file-saver": "^1.3.8",
    "marked": "^0.3.19",
    "ng-github-button": "^1.0.0",
    "ng-zorro-antd": "^1.8.0",
    "ng2-ace-editor": "^0.3.8",
    "ngx-color": "^1.3.1",
    "ngx-countdown": "^3.0.0",
    "ngx-highlight-js": "^2.0.0",
    "ngx-simplemde": "^1.1.1",
    "ngx-tinymce": "^2.0.0",
    "ngx-ueditor": "^2.1.0",
    "qrious": "^4.0.2",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26",
    "@webcomponents/custom-elements": "^1.2.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular-devkit/schematics": "~0.8.0",
    "@angular/cli": "~6.2.1",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@angularclass/hmr": "^2.1.3",
    "@bazel/ibazel": "^0.5.0",
    "@schematics/schematics": "~0.8.0",
    "@types/chalk": "^0.4.31",
    "@types/deep-extend": "^0.4.31",
    "@types/extend": "^3.0.0",
    "@types/fs-extra": "^5.0.1",
    "@types/htmlparser2": "^3.7.31",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jszip": "^3.1.3",
    "@types/mockjs": "^1.0.0",
    "@types/node": "~8.9.4",
    "@types/parse5": "^5.0.0",
    "antd-theme-generator": "^1.1.3",
    "chalk": "^1.1.3",
    "codecov": "^3.1.0",
    "codelyzer": "~4.3.0",
    "conventional-changelog-cli": "^2.0.5",
    "domhandler": "^2.4.2",
    "editorconfig-tools": "^0.1.1",
    "fs-extra": "^7.0.0",
    "gh-pages": "^1.2.0",
    "htmlparser2": "^3.9.2",
    "husky": "^1.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jsonml.js": "^0.1.0",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "klaw-sync": "^5.0.0",
    "less-plugin-clean-css": "^1.5.1",
    "lint-staged": "^7.1.0",
    "mark-twain": "^2.0.3",
    "mockjs": "^1.0.1-beta3",
    "mustache": "^2.3.0",
    "ng-packagr": "^4.1.1",
    "parse5": "^5.0.0",
    "prettier": "^1.14.1",
    "prettier-stylelint": "^0.4.2",
    "protractor": "~5.4.0",
    "readline-sync": "^1.4.9",
    "rollup": "^0.58.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^5.0.0",
    "stream": "0.0.2",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "~2.9.2",
    "webpack-bundle-analyzer": "^2.13.0",
    "xlsx": "^0.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "node ./scripts/git/commit-msg.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "packages/**/*.less": [
      "bash scripts/ci/lint.sh -t less",
      "git add"
    ],
    "packages/**/*.ts": [
      "bash scripts/ci/lint.sh -t ts",
      "git add"
    ]
  }
}
