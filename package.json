{
  "name": "delon",
  "version": "8.7.3",
  "description": "Delon is a set of essential modules for NG-ALAIN.",
  "keywords": [
    "delon",
    "ng-alain",
    "alain",
    "antd",
    "ng-zorro-antd",
    "angular"
  ],
  "author": "cipchk <cipchk@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ng-alain/ng-alain/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng-alain/delon.git"
  },
  "homepage": "https://ng-alain.com",
  "scripts": {
    "lint": "bash scripts/ci/lint.sh -ts -less",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "test": "ng test --code-coverage --watch=false",
    "test:cli": "bash scripts/ci/build-schematics.sh -b -t -travis -integration",
    "build": "bash scripts/ci/build.sh",
    "build_artifacts": "bash scripts/ci/build-artifacts.sh",
    "site:gen": "node scripts/site/main init && node scripts/build/generate-only-color.js",
    "site": "npm run site:gen && ng s -o",
    "site:build": "npm run site:gen && node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod --base-href /",
    "site:gh": "npm run site:build && gh-pages -d src/dist",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --pkg package.json",
    "pre-publish": "node scripts/publish/publish.js && node scripts/publish/publish-scaffold.js",
    "publish:next": "bash scripts/publish/publish.sh -next && npm run site:gh",
    "publish": "bash scripts/publish/publish.sh && npm run site:gh",
    "sync-scaffold-version": "node scripts/publish/sync-scaffold-version.js"
  },
  "dependencies": {
    "@angular/animations": "~8.2.11",
    "@angular/common": "~8.2.11",
    "@angular/compiler": "~8.2.11",
    "@angular/core": "~8.2.11",
    "@angular/forms": "~8.2.11",
    "@angular/platform-browser": "~8.2.11",
    "@angular/platform-browser-dynamic": "~8.2.11",
    "@angular/router": "~8.2.11",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1",
    "@angular/elements": "~8.2.11",
    "@antv/data-set": "^0.10.2",
    "@antv/g2": "^3.5.11",
    "@antv/g2-plugin-slider": "^2.1.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@stackblitz/sdk": "^1.3.0",
    "ajv": "^6.10.2",
    "extend": "^3.0.2",
    "file-saver": "^2.0.2",
    "marked": "^0.7.0",
    "ng-github-button": "^2.1.4",
    "ng-zorro-antd": "^8.5.2",
    "ng2-ace-editor": "^0.3.9",
    "ngx-color": "^4.1.0",
    "ngx-countdown": "^8.0.2",
    "ngx-highlight-js": "^2.1.1",
    "ngx-simplemde": "^1.1.1",
    "ngx-tinymce": "^7.0.0",
    "ngx-ueditor": "^2.1.3",
    "qrious": "^4.0.2",
    "@webcomponents/custom-elements": "^1.3.0",
    "aos": "^3.0.0-beta.6",
    "docsearch.js": "^2.6.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.14",
    "@angular/cli": "~8.3.14",
    "@angular/compiler-cli": "~8.2.11",
    "@angular/language-service": "~8.2.11",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "@types/aos": "^3.0.2",
    "@schematics/schematics": "^0.803.15",
    "@angularclass/hmr": "^2.1.3",
    "@types/deep-extend": "^0.4.31",
    "@types/extend": "^3.0.1",
    "@types/fs-extra": "^8.0.1",
    "@types/htmlparser2": "^3.10.1",
    "@types/jszip": "^3.1.6",
    "@types/mockjs": "^1.0.2",
    "@types/parse5": "^5.0.2",
    "karma-summary-reporter": "^1.7.1",
    "karma-parallel": "^0.3.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-language-service": "^0.9.9",
    "antd-theme-generator": "^1.1.7",
    "chalk": "^2.4.2",
    "codecov": "^3.6.1",
    "conventional-changelog-cli": "^2.0.25",
    "domhandler": "^3.0.0",
    "fs-extra": "^8.1.0",
    "gh-pages": "^2.1.1",
    "htmlparser2": "^4.0.0",
    "husky": "^3.0.9",
    "jsonml.js": "^0.1.0",
    "klaw-sync": "^6.0.0",
    "less-plugin-clean-css": "^1.5.1",
    "mark-twain": "^2.0.3",
    "mockjs": "^1.1.0",
    "mustache": "^3.1.0",
    "ng-packagr": "^5.7.1",
    "tsickle": "^0.37.0",
    "parse5": "^5.1.0",
    "prettier": "^1.18.2",
    "prettier-stylelint": "^0.4.2",
    "readline-sync": "^1.4.10",
    "stream": "0.0.2",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^3.1.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "xlsx": "^0.15.1",
    "screenfull": "^5.0.0",
    "less-bundle-promise": "^1.0.7",
    "lint-staged": "^8.2.1",
    "ng-alain-codelyzer": "^0.0.1",
    "ng-alain-sts": "^0.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./scripts/git/commit-msg.js -E HUSKY_GIT_PARAMS"
    }
  }
}
