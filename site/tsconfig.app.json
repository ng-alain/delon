{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"sourceMap": false,
		"declaration": false,
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"outDir": "../out-tsc/app",
		"target": "es5",
		"module": "es2015",
		"baseUrl": ".",
		"types": [],
		"paths": {
            "@shared": [ "app/shared" ],
            "@shared/*": [ "app/shared/*" ],
            "@delon/*": [ "../packages/*" ]
		}
	},
	"exclude": ["test.ts", "**/*.spec.ts", "templates/*.ts"]
}
