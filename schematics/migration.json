{
  "$schema": "./node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "migration-v17": {
      "version": "17",
      "description": "Updates NG-ALAIN to v18 [https://github.com/ng-alain/ng-alain/issues/2502]",
      "factory": "./ng-update/index#updateToV18"
    },
    "ng-post-update": {
      "description": "Performs cleanup after ng-update.",
      "factory": "./ng-update/index#postUpdate",
      "private": true
    }
  }
}
