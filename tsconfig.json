{
    "compileOnSave": false,
    "compilerOptions": {
      "outDir": "./dist/out-tsc",
      "sourceMap": true,
      "declaration": false,
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "target": "es5",
      "typeRoots": [
        "node_modules/@types"
      ],
      "lib": [
        "es2017",
        "dom"
      ],
      "baseUrl": "src/",
      "paths": {
        "@delon/*": [ "./core/*" ]
      },
      "plugins": [
        { "name": "tslint-language-service"}
      ]
    }
  }
